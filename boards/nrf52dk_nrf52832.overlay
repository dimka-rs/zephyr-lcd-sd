sd_spi: &spi2 {
    compatible = "nordic,nrf-spi";
    status = "okay";
    sck-pin = <25>;
    mosi-pin = <23>;
    miso-pin = <24>;
    cs-gpios = <&gpio0 22 GPIO_ACTIVE_LOW>;

    sdhc0: sdhc@0 {
        compatible = "zephyr,mmc-spi-slot";
        reg = <0>;
        status = "okay";
        label = "SDHC0";
        spi-max-frequency = <16000000>;
	};
};
